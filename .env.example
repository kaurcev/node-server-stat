# Конфигурация сервера
PORT=7787

# Интервал сбора статистики в миллисекундах
STATS_INTERVAL=1000

# Количество точек данных для сохранения в истории
HISTORY_LENGTH=60

# Сетевой интерфейс для мониторинга (оставьте пустым для автоматического определения)
# Пример: NETWORK_INTERFACE=eth0
NETWORK_INTERFACE=

# Количество основных процессов для отображения
PROCESSES_LIMIT=4

# Пользовательский путь к веб-сокету (необязательно)
# Пример: WS_PATH=/stats/ws
WS_PATH=